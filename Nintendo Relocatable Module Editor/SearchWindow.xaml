<Window x:Class="Nintendo_Relocatable_Module_Editor.SearchWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nintendo_Relocatable_Module_Editor"
        mc:Ignorable="d"
        Title="Search" Height="110" Width="300" ResizeMode="NoResize">
    <Grid>
        <Label Content="Find:" Margin="10,10,0,0" RenderTransformOrigin="1.24,0.667" Height="24" VerticalAlignment="Top" HorizontalAlignment="Left" Width="50"/>
        <TextBox x:Name="SearchBox" Margin="60,14,0,0" Height="20" VerticalAlignment="Top" HorizontalAlignment="Left" Width="214"/>
        <Button x:Name="Next" Width="70" Content="Next" Margin="204,39,0,0" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Click="Next_Click"/>
        <Button x:Name="Previous" Width="70" Content="Previous" Margin="10,39,0,0" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Click="Previous_Click"/>
    </Grid>
</Window>
